{
    "dataRoles": [
        {
            "displayName": "X Axis",
            "name": "x",
            "kind": "Grouping"
        },
        {
            "displayName": "Y Axis",
            "name": "y",
            "kind": "Grouping"
        },
        {
            "displayName": "Z Axis",
            "name": "z",
            "kind": "Grouping"
        },
        {
            "displayName": "Legend",
            "name": "legend",
            "kind": "Grouping"
        }
    ],
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "x"
                            }
                        },
                        {
                            "for": {
                                "in": "y"
                            }
                        },
                        {
                            "for": {
                                "in": "z"
                            }
                        },
                        {
                            "for": {
                                "in": "legend"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "window": {
                            "count": 30000
                        }
                    }
                }
            },
            "conditions": [
                {
                    "x": {
                        "max": 1
                    },
                    "y": {
                        "max": 1
                    },
                    "z": {
                        "max": 1
                    },
                    "legend": {
                        "max": 1
                    }
                }
            ]
        }
    ],
    
    "objects": {
        "axis": {
            "properties": {
                "revertXAxis": {
                    "type": {
                        "bool": true
                    }
                },
                "revertYAxis": {
                    "type": {
                        "bool": true
                    }
                },
                "revertZAxis": {
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "legend": {
            "properties": {
                "legendOrientation": {
                    "type": {
                        "enumeration": [
                            {
                                "value": "h",
                                "displayName": "Line"
                            },
                            {
                                "value": "l",
                                "displayName": "Column"
                            }
                        ]
                    }
                },
                "legendPosition": {
                    "type": {
                        "enumeration": [
                            {
                                "value": "0",
                                "displayName": "Left"
                            },
                            {
                                "value": "1",
                                "displayName": "Right"
                            }
                        ]
                    }
                },
                "show": {
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "style": {
            "properties": {
                "elementStyle": {
                    "type": {
                        "enumeration": [
                            {
                                "value": "markers",
                                "displayName": "Scatter"
                            },
                            {
                                "value": "lines",
                                "displayName": "Line"
                            },
                            {
                                "value": "lines+markers",
                                "displayName": "Line & Scatter"
                            }
                        ]
                    }
                },
                "lineSize": {
                    "type": {
                        "integer": true
                    }
                },
                "markerSize": {
                    "type": {
                        "integer": true
                    }
                },
                "displayLabels": {
                    "type": {
                        "bool": true
                    }
                },
                "labelsPosition": {
                    "type": {
                        "enumeration": [
                            {
                                "value": "middle",
                                "displayName": "Middle"
                            },
                            {
                                "value": "top",
                                "displayName": "Top"
                            },
                            {
                                "value": "bottom",
                                "displayName": "Bottom"
                            }
                        ]
                    }
                }
            }
        },
        "fetchMoreData": {
            "properties": {
                "activate": {
                    "type": {
                        "bool": true
                    }
                },
                "displayVisual": {
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    },
    "privileges": []
}