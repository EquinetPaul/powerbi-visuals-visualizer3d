{
    "dataRoles": [
        {
            "displayName": "Category Data",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Measure Data",
            "name": "measure",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "Measure Data 2",
            "name": "measure2",
            "kind": "GroupingOrMeasure"
        }
    ],
    "objects": {
        "dataPoint": {
            "properties": {
                "colorSelector": {
                    "properties": {
                        "fill": {
                            "type": {
                                "fill": {
                                    "solid": {
                                        "color": true
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "category"
                            }
                        },
                        {
                            "for": {
                                "in": "measure"
                            }
                        },
                        {
                            "for": {
                                "in": "measure2"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "privileges": []
}